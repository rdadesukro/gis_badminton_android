<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.projek_kp_gis_badminton_2021">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_MULTICAST_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Projek_KP_GIS_Badminton_2021"
        android:usesCleartextTraffic="true">
        <activity android:name=".menu.menu_petunjuk"
            android:label="PETUNJUK"></activity>
        <activity android:name=".menu.menu_tentang_aplikasi"
            android:label="ABOUT"/>
        <activity android:name=".menu.menu_marker"
            android:label="MARKER"/>
        <activity android:name=".menu.menu_tambah_foto" />
        <activity
            android:name=".menu.menu_add_foto"
            android:label="TAMBAH FOTO" />
        <activity
            android:name=".menu.menu_detail"
            android:label="DETAIL LAPANGAN" />
        <activity
            android:name=".menu.menu_tambah_lapngan"
            android:label="TAMBAH LAPANGAN" />
        <activity android:name=".menu.menu_utama" />
        <activity
            android:name=".menu.menu_jenis_lapangan"
            android:label="JENIS LAPANGAN" />
        <activity android:name=".menu.menu_utama_pemilik" />
        <activity android:name=".menu.menu_update_password" />
        <activity android:name=".menu.menu_riset_password" />
        <activity
            android:name=".menu.menu_register"
            android:windowSoftInputMode="stateVisible|adjustResize" />
        <activity
            android:name=".menu.menu_tambah_jenis"
            android:label="TAMABAH JENIS LAPANGAN"
            android:windowSoftInputMode="stateVisible|adjustResize" />
        <activity
            android:name=".menu.menu_lapangan"
            android:label="LAPANGAN" />
        <activity
            android:name=".menu.fragment.menu_profil"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".menu.fragment.menu_home"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".menu.menu_login"
            android:windowSoftInputMode="stateVisible|adjustResize" />
        <activity android:name=".MainActivity" />
        <activity android:name=".menu.menu_pembuka">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>